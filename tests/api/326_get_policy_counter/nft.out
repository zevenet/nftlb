table netdev nftlb {
	set black001 {
		type ipv4_addr
		flags interval
		counter
		auto-merge
		elements = { 35.195.60.76 counter packets 0 bytes 0, 35.195.60.77 counter packets 0 bytes 0,
			     35.195.60.78 counter packets 0 bytes 0, 127.0.0.1 counter packets 2 bytes 120 }
	}

	map proto-services-lo {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 80 : goto farml7 }
	}

	set black001-farml7-cnt {
		type ipv4_addr
		size 65535
		flags dynamic,timeout
		counter
		timeout 2m
		elements = { 127.0.0.1 counter packets 2 bytes 120 timeout 2m expires 1m57s952ms }
	}

	chain ingress-lo {
		type filter hook ingress device "lo" priority 101; policy accept;
		ip protocol . ip daddr . th dport vmap @proto-services-lo
	}

	chain farml7 {
		ip saddr @black001 add @black001-farml7-cnt { ip saddr } log prefix "policy-BL-black001-farml7 " drop
	}
}
table ip nftlb {
	map nat-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
	}

	map proto-services-back-m {
		type mark : ipv4_addr
		elements = { 0x00000203 : 192.168.100.212, 0x00000512 : 127.0.0.1, 0x00000513 : 192.168.101.210 }
	}

	map filter-proto-services {
		type inet_proto . ipv4_addr . inet_service : verdict
		elements = { tcp . 127.0.0.1 . 80 : goto filter-farml7 }
	}

	chain prerouting {
		type nat hook prerouting priority dstnat; policy accept;
		ip protocol . ip daddr . th dport vmap @nat-proto-services
	}

	chain postrouting {
		type nat hook postrouting priority srcnat; policy accept;
		ct mark 0x00000000 ct mark set meta mark
		ct mark 0x80000000/1 masquerade
		snat to ct mark map @proto-services-back-m
	}

	chain filter {
		type filter hook prerouting priority mangle; policy accept;
		meta mark set ct mark
		ip protocol . ip daddr . th dport vmap @filter-proto-services
	}

	chain filter-farml7 {
	}
}
